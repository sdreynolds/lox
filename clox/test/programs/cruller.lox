class Doughnut {
  cook() {
    print "Dunk in the fryer.";
    this.finish("sprinkles");
  }

  finish(ingredient) { print "Finish with " + ingredient; }  
}

class Cruller < Doughnut {
  finish(ingredient) {
    // ignore ingredient and finish with icing
    super.finish("icing");
  }
}

Cruller().cook();
